<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" charset="utf-8" initial-scale="1.0" />
    <title>@ViewData["Title"] - Challenge_Superliga</title>
    <link href="~/css/_Layout/_Layout.css" rel="stylesheet" />
    <link href="~/css/Normalize.css" rel="stylesheet" />
    <link href="~/lib/DataTable/datatables.css" rel="stylesheet" />
    <link href="~/lib/Boostrap/css/bootstrap.css" rel="stylesheet" />
    <script src="~/lib/jquery/Jquery.js"></script>
    <script src="~/lib/DataTable/datatables.js"></script>
    <link href='https://unpkg.com/boxicons@2.0.9/css/boxicons.min.css' rel='stylesheet'>
    <script src="https://cdn.jsdelivr.net/npm/typed.js@2.0.12"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-/bQdsTh/da6pkI1MST/rWKFNjaCP5gBSY4sEBT38Q/9RBh9AH40zEOg7Hlq2THRZ" crossorigin="anonymous"></script>
    <script src="~/lib/Boostrap/js/bootstrap.js"></script>
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-lg navbar-dark p-md-3">
            <div class="container">
                <span class="typing" translate="no"></span>
                <button class="navbar-toggler"
                        type="button"
                        data-bs-toggle="collapse"
                        data-bs-target="#navbarNav"
                        aria-controls="navbarNav"
                        aria-expanded="false"
                        aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarNav">
                    <div class="mx-auto"></div>
                    <ul class="navbar-nav">
                        <li id="nav-itemh" class="nav-item pl-2 fs-3"><a><span data-content="Home" aria-hidden="true"></span>@Html.ActionLink("Home", "Index", "Home")</a></li>
                        <li id="nav-items" class="nav-item pl-2 fs-3"><a>@Html.ActionLink("Socio", "Socios", "Socios")</a></li>
                    </ul>
                </div>
            </div>
        </nav>       
    </header>
    <div class="container">
        <main role="main">
            @RenderBody()
        </main>
    </div>
    <footer id="footer" class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - Challenge_Superliga
        </div>
    </footer>
    <script>
        $(document).ready(function () {
            var typed = new Typed(".typing", {
                strings: ["SuperLiga"],
                typeSpeed: 100,
                backSpeed: 60,
                loop: true
            });
        });

    </script>
</body>
</html>

